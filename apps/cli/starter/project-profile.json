{
  "schemaVersion": "1.0.0",
  "kind": "palace/project-profile",
  "projectRoot": ".",
  "languages": ["unknown"],
  "capabilities": {
    "search.text": {
      "command": "rg --no-heading --line-number --color never \"{{query}}\" {{paths}}",
      "description": "Text search via ripgrep"
    },
    "read.file": {
      "command": "cat {{path}}",
      "description": "Read a single file"
    },
    "graph.deps": {
      "command": "echo graph deps not configured",
      "description": "List project dependencies"
    },
    "tests.run": {
      "command": "echo tests not configured",
      "description": "Run project tests"
    },
    "lint.run": {
      "command": "echo lint not configured",
      "description": "Run project linter"
    },
    "symbols.lookup": {
      "command": "echo symbols lookup not configured",
      "description": "Stub symbol lookup"
    }
  },
  "provenance": {
    "createdBy": "palace init",
    "createdAt": "{{createdAt}}"
  }
}
